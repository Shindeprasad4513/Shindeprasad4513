SLIP30
#include<stdio.h>
#include<string.h>
struct emp
{
	int id;
	char name[20];
	float salary;
}e[100];

int main()
{
	int n,i;
	float max=0;
	printf("Enter limit:");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		printf("\nEnter employee id::");
		scanf("%d",&e[i].id);
		printf("\nEnter employee name::");
		scanf("%s",e[i].name);
		printf("\nEnter employee salary::");
		scanf("%f",&e[i].salary);
	}
		max=e[0].salary;
		
	for(i=0;i<n;i++)
	{
		if(e[i].salary>max)
		{
			max=e[i].salary;
		}
	}
	printf("Maximum salary is::%f",max);
}
