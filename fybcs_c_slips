SLIP14
#include<stdio.h>
#include<stdlib.h>
int main()
{
	FILE *f1,*f2;
	char ch;
	int i;
	f1=fopen("student.txt","r");
	f2=fopen("temp.txt","w");
	
	if(f1==NULL)
	{
		printf("file does not exist!!");
		exit(0);
	}
	while(!feof(f1))
	{
		ch=getc(f1);
		if(ch>='A'&&ch<='Z')
		{
			ch=ch+32;
			fputc(ch,f2);
		}
		else if(ch>='a'&&ch<='z')
		{
			ch=ch-32;
			fputc(ch,f2);
		}
		else if(ch>='0'&&ch<='9')
		{
			ch='*';
			fputc(ch,f2);
		}
		else
		{
			fputc(ch,f2);
		}
		
	}
	printf("File copieed successfully!!!!");
	fclose(f1);
	fclose(f2);
}
