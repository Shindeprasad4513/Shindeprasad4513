void dispemp();
struct emp
{
	int eno;
	char ename[20];
	float salary;
}e[100];

void dispemp(struct emp e[],int n,int i)
{
	for(i=0;i<n;i++)
	{
		if(e[i].salary>=10000)
		{
			printf("\n employee no::%d",e[i].eno);
			printf("\n employee name::%s",e[i].ename);
			printf("\n employee salary::%f",e[i].salary);
		}
	}
}
