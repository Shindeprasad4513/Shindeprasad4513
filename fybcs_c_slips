{
	int n,i;
	printf("\nEnter limit:");
	scanf("%d",&n);
	
	for(i=0;i<n;i++)
	{
		printf("Enter employee no::");
		scanf("%d",&e[i].eno);
		printf("Enter employee name::");
		scanf("%s",e[i].ename);
		printf("Enter employee salary::");
		scanf("%f",&e[i].salary);
	}
	dispemp(e,n,i);
}
