Write a program to accept details of n employees (id, name, salary). Display all the details. Also, search for an employee by name.*/
#include<stdio.h>
struct emp
{
	int id,salary;
	char ename[10];
}e[10];

int main()
{
	int n,i;
	char name[20];
	printf("Enter limit::");
	scanf("%d",&n);
	
	for(i=0;i<n;i++)
	{
		printf("Enter employee id::");
		scanf("%d",&e[i].id);
		printf("Enter employee name::");
		scanf("%s",e[i].ename);
		printf("Enter employee salary::");
		scanf("%d",&e[i].salary);
	}
	for(i=0;i<n;i++)
	{
		printf("\nemployee id::%d",e[i].id);
		printf("\nemployee name::%s",e[i].ename);
		printf("\nemployee salary::%d",e[i].salary);
	}
	
	printf("Enter name to search:");
	fgets(name,20,stdin);
	for(i=0;i<n;i++)
	{
		if(e[i].ename==name)
		{
			printf("\nemployee id::%d",e[i].id);
			printf("\nemployee name::%s",e[i].ename);
			printf("\nemployee salary::%d",e[i].salary);
			
		}
	}
	
}
