SLIP 26
#include<stdio.h>
struct emp
{
	int id;
	char name[20];
	float salary;
}e[100];

int main()
{
	int n,i,ch;
	printf("Enter limit:");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		printf("\nEnter employee id::");
		scanf("%d",&e[i].id);
		printf("\nEnter employee name::");
		scanf("%s",e[i].name);
		printf("\nEnter employee salary::");
		scanf("%f",&e[i].salary);
		
	}
	do{
		printf("\n MENU::\n1.Search by name.\n2.Display all employee.");
		printf("\nEnter your choice:");
		scanf("%d",&ch);
		
		switch(ch)
		{
			case 1:char ename[20];
					printf("\nenter name to search:");
					scanf("%s",ename);
					
					for(i=0;i<n;i++)
					{
						if(strcmp(e[i].name,ename)==1)
						{
								printf("\n employee id::%d",e[i].id);
								printf("\n employee name::%s",e[i].name);
								printf("\n employee salary::%f",e[i].salary);
								
						}
					}
			
			case 2:for(i=0;i<n;i++)
					{
								printf("\n employee id::%d",e[i].id);
								printf("\n employee name::%s",e[i].name);
								printf("\n employee salary::%f",e[i].salary);
					}
		}
	}while(ch!=3);
}
